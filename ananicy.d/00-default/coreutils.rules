# Some rules for computationally or IO expensive tools from the GNU coreutils
NAME=cat        TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=cksum      TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=comm       TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=dd         TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=df         TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=du         TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=install    TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=join       TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=md5sum     TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=mkdir      TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=mv         TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=rm         TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=rmdir      TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sha1sum    TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sha256sum  TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sha384sum  TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sha512sum  TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=shred      TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sort       TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=sum        TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=tac        TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=tee        TYPE=BG_CPUIO  PGREP_ARGS='-wx'
NAME=wc         TYPE=BG_CPUIO  PGREP_ARGS='-wx'